{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "autobuild": "rollup --watch --config rollup.config.js",
    "build": "rollup --config rollup.config.js",
    "build:fontawesome": "mkdir -p dist/assets/third-party/@fortawesome/fontawesome-free/webfonts && mkdir -p dist/assets/third-party/@fortawesome/fontawesome-free/css && cp -a ./node_modules/@fortawesome/fontawesome-free/webfonts dist/assets/third-party/@fortawesome/fontawesome-free/ && cp ./node_modules/@fortawesome/fontawesome-free/css/all.min.css dist/assets/third-party/@fortawesome/fontawesome-free/css/all.min.css",
    "build:tailwind": "tailwind build dist/assets/styles/index.css -o dist/assets/styles/tailwind.css",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf dist/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run build",
    "start": "sirv dist --single --host=localhost --port=5000",
    "start:frontend-only": "cross-env FRONTEND_ONLY_MODE=true rollup --watch --config rollup.config.js",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "@opentelemetry/api": "^0.12.0",
    "@opentelemetry/exporter-jaeger": "^0.12.0",
    "@opentelemetry/plugin-document-load": "^0.13.1",
    "@opentelemetry/tracing": "^0.12.0",
    "@opentelemetry/web": "^0.12.0",
    "@popperjs/core": "2.5.1",
    "@tailwindcss/custom-forms": "0.2.1",
    "@types/chart.js": "^2.9.25",
    "@types/faker": "^5.1.2",
    "axios": "^0.21.1",
    "chart.js": "2.9.3",
    "dayjs": "^1.9.1",
    "factory.ts": "^0.5.1",
    "faker": "^5.1.0",
    "sirv-cli": "^1.0.6",
    "string-format": "^2.0.0",
    "svelte-click-outside": "^1.0.0",
    "svelte-json-tree": "^0.1.0",
    "svelte-routing": "^1.4.2",
    "svelte-select": "^3.14.1",
    "svelte-simple-modal": "^0.6.1",
    "sveltejs-forms": "^2.1.2",
    "tailwindcss": "1.8.10",
    "typed-intl": "^1.0.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/string-format": "^2.0.0",
    "@types/valid-url": "^1.0.3",
    "@types/yup": "^0.29.10",
    "cross-env": "^7.0.2",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-svelte": "^1.4.1",
    "prettier-plugin-tailwind": "^2.2.2",
    "rollup": "^2.3.4",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  }
}

{
	"defaultSeverity": "warning",
	"extends": ["tslint:recommended"],
	"linterOptions": {
		"exclude": ["node_modules/**"]
	},
	"rules": {
		"no-console": false,
		"quotemark": [true, "double"],
		"indent": [true, "tabs", 1],
		"interface-name": false,
		"ordered-imports": true,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false
	}
}

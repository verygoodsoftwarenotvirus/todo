{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s",
    "build:tailwind": "tailwind build public/assets/styles/index.css -o public/assets/styles/tailwind.css",
    "build:fontawesome": "mkdir -p public/assets/third-party/@fortawesome/fontawesome-free/webfonts && mkdir -p public/assets/third-party/@fortawesome/fontawesome-free/css && cp -a ./node_modules/@fortawesome/fontawesome-free/webfonts public/assets/third-party/@fortawesome/fontawesome-free/ && cp ./node_modules/@fortawesome/fontawesome-free/css/all.min.css public/assets/third-party/@fortawesome/fontawesome-free/css/all.min.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run dev",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "rollup": "2.28.1",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.0.1",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.26.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "typescript": "^3.9.3",
    "@rollup/plugin-typescript": "^4.0.0",
    "tslib": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "@popperjs/core": "2.5.1",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "@types/chart.js": "^2.9.24",
    "chart.js": "2.9.3",
    "sirv-cli": "1.0.6",
    "svelte-routing": "1.4.2",
    "tailwindcss": "1.8.10"
  }
}
